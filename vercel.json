{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/revalidate.ts": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/revalidate",
      "schedule": "0 */12 * * *"
    }
  ]
}